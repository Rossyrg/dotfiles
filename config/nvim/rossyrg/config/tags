-	shortcuts/search.vim	/^noremap - ;$/;"	m
<A-Down>	shortcuts/fold.vim	/^nnoremap <A-Down> zo$/;"	m
<A-Left>	shortcuts/fold.vim	/^nnoremap <A-Left> zm$/;"	m
<A-Right>	shortcuts/fold.vim	/^nnoremap <A-Right> zr$/;"	m
<A-S-Down>	shortcuts/fold.vim	/^nnoremap <A-S-Down> zO$/;"	m
<A-S-Left>	shortcuts/fold.vim	/^nnoremap <A-S-Left> zM$/;"	m
<A-S-Right>	shortcuts/fold.vim	/^nnoremap <A-S-Right> zR$/;"	m
<A-S-Up>	shortcuts/fold.vim	/^nnoremap <A-S-Up> zC$/;"	m
<A-Up>	shortcuts/fold.vim	/^nnoremap <A-Up> zc$/;"	m
<C-Down>	shortcuts/fold.vim	/^nnoremap <C-Down> zj$/;"	m
<C-S-Down>	shortcuts/fold.vim	/^nnoremap <C-S-Down> ]]$/;"	m
<C-S-Up>	shortcuts/fold.vim	/^nnoremap <C-S-Up> [[$/;"	m
<C-Up>	shortcuts/fold.vim	/^nnoremap <C-Up> zk$/;"	m
